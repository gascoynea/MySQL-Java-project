<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.AppointmentSearch">
   <children>
      <TableView fx:id="tableView" layoutY="49.0" prefHeight="344.0" prefWidth="1058.0">
        <columns>
          <TableColumn fx:id="appointmentIDCol" prefWidth="97.0" text="Appointment ID" />
          <TableColumn fx:id="titleCol" prefWidth="150.0" text="Title" />
            <TableColumn fx:id="descriptionCol" prefWidth="101.0" text="Description" />
            <TableColumn fx:id="locationCol" prefWidth="103.0" text="Location" />
            <TableColumn fx:id="contactCol" prefWidth="92.0" text="Contact" />
            <TableColumn fx:id="typeCol" prefWidth="88.0" text="Type" />
            <TableColumn fx:id="startDTCol" prefWidth="141.0" text="Start Date and Time" />
            <TableColumn fx:id="endDTCol" prefWidth="133.0" text="End Date and Time" />
            <TableColumn fx:id="customerIDCol" prefWidth="84.0" text="Customer_ID" />
            <TableColumn fx:id="userIDCol" minWidth="0.0" prefWidth="68.0" text="User_ID" />
        </columns>
      </TableView>
      <RadioButton fx:id="monthlyRB" layoutX="496.0" layoutY="32.0" mnemonicParsing="false" onAction="#onMonthlyRBSelected" text="Monthly" />
      <RadioButton fx:id="biWeeklyRB" layoutX="496.0" layoutY="6.0" mnemonicParsing="false" onAction="#onBiWeeklyRBSelected" text="Biweekly" />
      <Button fx:id="mainAppointmentButton" mnemonicParsing="false" onAction="#mainAppointmentButtonClick" text="Main appointment Window" />
   </children>
</AnchorPane>
